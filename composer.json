{
    "name": "sfu_dhil/amp",
    "description": "Amplify Podcast Network",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/cache": "^2.1",
        "doctrine/common": "^3.2",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.7",
        "knplabs/knp-menu-bundle": "^3.2",
        "knplabs/knp-paginator-bundle": "^5.8",
        "league/csv": "^9.6",
        "lukaswhite/podcast-feed-parser": "dev-main#7ebe0032e23e1202624c5853017835e81fe03d3c",
        "ninsuo/symfony-collection": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "scienta/doctrine-json-functions": "^4.1",
        "sensio/framework-extra-bundle": "^6.2",
        "sfu-dhil/nines": "dev-4.x-up#33efcc5851363c95be6f7bf5ae4af4c2467e662c",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.17.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/messenger": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/yaml": "4.4.*",
        "tetranz/select2entity-bundle": "^3.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "deployer/deployer": "^6.8",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.4",
        "friendsoftwig/twigcs": "^5.1",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-bundle": "4.4.*",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*",
        "zenstruck/messenger-test": "^1.5"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/sfu-dhil/nines-bundles.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sfu-dhil/podcast-feed-parser.git"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "Fuz\\Symfony\\Collection\\ScriptHandler::postInstall"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "Fuz\\Symfony\\Collection\\ScriptHandler::postUpdate"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/thanks": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
